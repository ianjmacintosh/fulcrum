{
  "name": "fulcrum",
  "version": "1.0.0",
  "description": "Understand your job search like a pro",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "db:seed": "tsx src/db/seed.ts",
    "db:migrate": "tsx src/db/migrations/migration-runner.ts",
    "db:backup": "node scripts/backup-db.js",
    "db:restore": "node scripts/restore-db.js",
    "db:test": "tsx test-connection.js",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tanstack/react-router": "^1.130.12",
    "@tanstack/react-start": "^1.130.16",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "mongodb": "^6.18.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "vite": "^6.3.5",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
